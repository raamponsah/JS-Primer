// Write a function that accepts an array of numbers 
// and returns a new array of odd numbers only.


function determineOddNumbers(a){
    let oddArray = []
    for (let i = 0; i < a.length; i++) {
            if(a[i] % 2 == 1){
                oddArray.push(a[i])
                
            }
        
    }

    return oddArray
}

determineOddNumbers([3,4,8,12,13])