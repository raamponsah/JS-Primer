// 3. Write a function that accepts two arrays and display them in the console.

function displayTwoArrayList(a,b){

    for(i=0; i<a.length;i++){
        console.log(a[i])
    }

      for(i=0; i<b.length;i++){
        console.log(b[i])
      }


    
}

// More elegant
function displayTwoArrayList2(a,b){
    let c = a.concat(b)
      for(i=0; i<c.length;i++){
        console.log(c[i])
    }

}

displayTwoArrayList([2,3,4],[5,6,7])
